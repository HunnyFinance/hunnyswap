// Trading Chef calculates trading fees and reward XOXO tokens
// He calculates all value in FTM, uses WFTM as anchor token
contract TradingChef is Ownable {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    struct UserInfo {
        uint256 quantity; // How many LP tokens the user has provided
        uint256 blockNumber; // Last transaction block
    }

    struct PoolInfo {
        address pair; // Trading pairs that can be mined
        uint256 quantity; // Current amount of LPs
        uint256 totalQuantity; // All quantity
        uint256 allocPoint; // How many allocation points assigned to this pool
        uint256 allocXoXoAmount; // How many XOXOs
        uint256 lastRewardBlock; // Last transaction block
    }

    EnumerableSet.AddressSet private _whitelist;

    // XOXO tokens created per block
    uint256 public xoxoPerBlock;

    // The block number when XOXO mining starts.
    uint256 public startBlock;

    // How many blocks are halved
    uint256 public halvingPeriod = 15768000; // 1/2 year

    // Total allocation points
    uint256 public totalAllocPoint = 0;

    ICalculator public calculator;

    // Router address
    address public router;

    // Factory address
    IUniswapV2Factory public factory;

    // XoXoToken address
    XoXoToken public xoxo;

    // Calculate price based on FTM
    // Should be WFTM address
    address public targetToken;

    uint256 public claimFeeMax = 98; // 98 %
    uint256 public claimFeePeriod = 7 days;

    // pair corresponding pid
    mapping(address => uint256) public pairOfPid;

    PoolInfo[] public poolInfo;
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // Last time user claim reward
    mapping(address => uint256) public lastClaimTime;

    modifier onlyRouter() {
        require(msg.sender == router, "TradingChef: caller is not the router");
        _;
    }

    constructor(
        XoXoToken _xoxo,
        IUniswapV2Factory _factory,
        ICalculator _calculator,
        address _router,
        address _targetToken,
        uint256 _xoxoPerBlock,
        uint256 _startBlock
    ) {
        require(address(_xoxo) != address(0), "illegal address");
        xoxo = _xoxo;

        require(address(_factory) != address(0), "illegal address");
        factory = _factory;

        require(address(_calculator) != address(0), "illegal address");
        calculator = _calculator;

        require(_router != address(0), "illegal address");
        router = _router;

        targetToken = _targetToken;
        xoxoPerBlock = _xoxoPerBlock;
        startBlock = _startBlock;
    }

    function poolLength() public view returns (uint256) {
        return poolInfo.length;
    }

    function addPair(
        uint256 _allocPoint,
        address _pair,
        bool _withUpdate
    ) public onlyOwner {
        require(_pair != address(0), "_pair is the zero address");
        if (_withUpdate) {
            massMintPools();
        }
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.push(
            PoolInfo({
                pair: _pair,
                quantity: 0,
                totalQuantity: 0,
                allocPoint: _allocPoint,
                allocXoXoAmount: 0,
                lastRewardBlock: lastRewardBlock
            })
        );
        pairOfPid[_pair] = poolLength() - 1;
    }

    // Update the allocPoint of the pool
    function setPair(
        uint256 _pid,
        uint256 _allocPoint,
        bool _withUpdate
    ) public onlyOwner {
        if (_withUpdate) {
            massMintPools();
        }

        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
    }

    // Set the number of XOXO produced by each block
    function setXoXoPerBlock(uint256 _xoxoPerBlock) public onlyOwner {
        massMintPools();
        xoxoPerBlock = _xoxoPerBlock;
    }

    // Only tokens in the whitelist can be mined XOXO
    function addWhitelist(address _addToken) public onlyOwner returns (bool) {
        require(_addToken != address(0), "TradingChef: token is the zero address");
        return EnumerableSet.add(_whitelist, _addToken);
    }

    function delWhitelist(address _delToken) public onlyOwner returns (bool) {
        require(_delToken != address(0), "TradingChef: token is the zero address");
        return EnumerableSet.remove(_whitelist, _delToken);
    }

    function getWhitelistLength() public view returns (uint256) {
        return EnumerableSet.length(_whitelist);
    }

    function isWhitelist(address _token) public view returns (bool) {
        return EnumerableSet.contains(_whitelist, _token);
    }

    function getWhitelist(uint256 _index) public view returns (address) {
        require(_index <= getWhitelistLength() - 1, "TradingChef: index out of bounds");
        return EnumerableSet.at(_whitelist, _index);
    }

    function setHalvingPeriod(uint256 _block) public onlyOwner {
        halvingPeriod = _block;
    }

    function setRouter(address newRouter) public onlyOwner {
        require(newRouter != address(0), "TradingChef: new router is the zero address");
        router = newRouter;
    }

    function setCalculator(ICalculator _calculator) public onlyOwner {
        require(address(_calculator) != address(0), "TradingChef: new calculator is the zero address");
        calculator = _calculator;
    }

    function phase(uint256 blockNumber) public view returns (uint256) {
        if (halvingPeriod == 0) {
            return 0;
        }
        if (blockNumber > startBlock) {
            return (blockNumber.sub(startBlock).sub(1)).div(halvingPeriod);
        }
        return 0;
    }

    function reward(uint256 blockNumber) public view returns (uint256) {
        uint256 _phase = phase(blockNumber);
        return xoxoPerBlock.div(2**_phase);
    }

    function reward() public view returns (uint256) {
        return reward(block.number);
    }

    function getXoXoReward(uint256 _lastRewardBlock) public view returns (uint256) {
        require(_lastRewardBlock <= block.number, "TradingChef: must little than the current block number");
        uint256 blockReward = 0;
        uint256 n = phase(_lastRewardBlock);
        uint256 m = phase(block.number);
        // If it crosses the cycle
        while (n < m) {
            n++;
            // Get the last block of the previous cycle
            uint256 r = n.mul(halvingPeriod).add(startBlock);
            // Get rewards from previous periods
            blockReward = blockReward.add((r.sub(_lastRewardBlock)).mul(reward(r)));
            _lastRewardBlock = r;
        }
        blockReward = blockReward.add((block.number.sub(_lastRewardBlock)).mul(reward(block.number)));
        return blockReward;
    }

    // Update all pools Called when updating allocPoint and setting new blocks
    function massMintPools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            mint(pid);
        }
    }

    function mint(uint256 _pid) public returns (bool) {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return false;
        }
        uint256 blockReward = getXoXoReward(pool.lastRewardBlock);
        if (blockReward <= 0) {
            return false;
        }
        // Calculate the rewards obtained by the pool based on the allocPoint
        uint256 xoxoReward = blockReward.mul(pool.allocPoint).div(totalAllocPoint);
        // Increase the number of tokens in the current pool
        pool.allocXoXoAmount = pool.allocXoXoAmount.add(xoxoReward);
        pool.lastRewardBlock = block.number;
        return true;
    }

    function swap(
        address account,
        address input,
        address output,
        uint256 amount
    ) public onlyRouter returns (bool) {
        require(account != address(0), "TradingChef: taker swap account is the zero address");
        require(input != address(0), "TradingChef: taker swap input is the zero address");
        require(output != address(0), "TradingChef: taker swap output is the zero address");

        if (poolLength() <= 0) {
            return false;
        }

        if (!isWhitelist(input) || !isWhitelist(output)) {
            return false;
        }

        address pair = factory.getPair(input, output);
        PoolInfo storage pool = poolInfo[pairOfPid[pair]];
        // If it does not exist or the allocPoint is 0 then return
        if (pool.pair != pair || pool.allocPoint <= 0) {
            return false;
        }

        uint256 quantity = getQuantity(output, amount, targetToken);
        if (quantity <= 0) {
            return false;
        }

        mint(pairOfPid[pair]);

        pool.quantity = pool.quantity.add(quantity);
        pool.totalQuantity = pool.totalQuantity.add(quantity);
        UserInfo storage user = userInfo[pairOfPid[pair]][account];
        user.quantity = user.quantity.add(quantity);
        user.blockNumber = block.number;

        if (lastClaimTime[account] == 0) {
            lastClaimTime[account] = block.timestamp;
        }

        return true;
    }

    function getQuantity(
        address outputToken,
        uint256 outputAmount,
        address anchorToken
    ) public view returns (uint256) {
        uint256 quantity = 0;
        if (outputToken == anchorToken) {
            quantity = outputAmount;
        } else if (factory.getPair(outputToken, anchorToken) != address(0)) {
            quantity = calculator.consult(outputToken, outputAmount, anchorToken);
        } else {
            uint256 length = getWhitelistLength();
            for (uint256 index = 0; index < length; index++) {
                address intermediate = getWhitelist(index);
                if (
                    factory.getPair(outputToken, intermediate) != address(0) &&
                    factory.getPair(intermediate, anchorToken) != address(0)
                ) {
                    uint256 interQuantity = calculator.consult(outputToken, outputAmount, intermediate);
                    quantity = calculator.consult(intermediate, interQuantity, anchorToken);
                    break;
                }
            }
        }
        return quantity;
    }

    function takerWithdraw() public {
        uint256 userSub;
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            PoolInfo storage pool = poolInfo[pid];
            UserInfo storage user = userInfo[pid][msg.sender];
            if (user.quantity > 0) {
                mint(pid);

                // The reward held by the user in this pool
                uint256 userReward = pool.allocXoXoAmount.mul(user.quantity).div(pool.quantity);
                pool.quantity = pool.quantity.sub(user.quantity);
                pool.allocXoXoAmount = pool.allocXoXoAmount.sub(userReward);
                user.quantity = 0;
                user.blockNumber = block.number;
                userSub = userSub.add(userReward);
            }
        }
        if (userSub <= 0) {
            return;
        }
        safeClaimXoXo(msg.sender, userSub);
    }

    function getUserReward(uint256 _pid, address _user) public view returns (uint256, uint256) {
        require(_pid <= poolInfo.length - 1, "TradingChef: Not find this pool");

        uint256 userSub;
        PoolInfo memory pool = poolInfo[_pid];
        UserInfo memory user = userInfo[_pid][_user];
        if (user.quantity > 0) {
            uint256 blockReward = getXoXoReward(pool.lastRewardBlock);
            uint256 xoxoReward = blockReward.mul(pool.allocPoint).div(totalAllocPoint);
            userSub = userSub.add((pool.allocXoXoAmount.add(xoxoReward)).mul(user.quantity).div(pool.quantity));
        }

        // XOXO available to users, User transaction amount
        return (userSub, user.quantity);
    }

    function getUserTotalReward(address _user) public view returns (uint256 userReward) {
        userReward = 0;
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            (uint256 userSub, ) = getUserReward(pid, _user);
            userReward = userReward.add(userSub);
        }
    }

    function getPoolInfo(uint256 _pid)
        public
        view
        returns (
            address,
            address,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        require(_pid <= poolInfo.length - 1, "TradingChef: Not find this pool");

        PoolInfo memory pool = poolInfo[_pid];
        address token0 = IUniswapV2Pair(pool.pair).token0();
        address token1 = IUniswapV2Pair(pool.pair).token1();
        uint256 xoxoAmount = pool.allocXoXoAmount;
        uint256 blockReward = getXoXoReward(pool.lastRewardBlock);
        uint256 xoxoReward = blockReward.mul(pool.allocPoint).div(totalAllocPoint);
        xoxoAmount = xoxoAmount.add(xoxoReward);

        //token0,token1,Pool remaining reward,Total /Current transaction volume of the pool
        return (token0, token1, xoxoAmount, pool.totalQuantity, pool.quantity, pool.allocPoint);
    }

    function ownerWithdraw(address _to, uint256 _amount) public onlyOwner {
        safeXoXoTransfer(_to, _amount);
    }

    function setClaimFee(uint256 _period, uint256 _maxFee) external onlyOwner {
        claimFeePeriod = _period;
        claimFeeMax = _maxFee;
    }

    function safeClaimXoXo(address _to, uint256 _amount) internal {
        // charge claim fees
        uint256 passedTime = block.timestamp.sub(lastClaimTime[_to]);
        if (passedTime < claimFeePeriod) {
            uint256 feeReduced = claimFeeMax.sub(claimFeeMax.mul(passedTime.mul(100).div(claimFeePeriod)).div(100));
            uint256 feeAmount = _amount.mul(feeReduced).div(100);
            _amount = _amount.sub(feeAmount);
        }

        lastClaimTime[_to] = block.timestamp;
        safeXoXoTransfer(_to, _amount);
    }

    function safeXoXoTransfer(address _to, uint256 _amount) internal {
        uint256 balance = xoxo.balanceOf(address(this));
        if (_amount > balance) {
            _amount = balance;
        }
        xoxo.transfer(_to, _amount);
    }
}
